#!groovy
pipeline {
    agent any
    stages {
		stage('Code checkout') {
			steps {
				deleteDir()
				checkout scm
            }
        }
        stage('dry run') {
			steps {
                sh 'sudo su'
                sh 'virtualenv --python=python2.7 custodian'
                sh 'source custodian/bin/activate'
                sh 'pip install c7n'
                sh 'custodian validate custodian.yml'
                sh 'custodian run --dryrun -s . ec2Stop.yml'

            }
        }
    }
}